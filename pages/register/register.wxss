   color: rgb(98, 98, 98);
   border-radius: 12rpx;
}
 .form-wrap .input-box.btn-box {
    position: absolute;
    display: block;
    bottom: 2rem;
    left: 2.8rem;
    right: 2.8rem;
    width: auto;
