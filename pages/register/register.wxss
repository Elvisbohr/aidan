    display: block;
    outline: none;
    border: 0;
}

.form-wrap .input-box .btn.disable {
    background: #a0a0a0;
} 


.cube {
    position: absolute;
    top: 0;
    border-radius: 5px;
    background: -ms-linear-gradient(left, #fd4b33, #ff6b42);
    background: -webkit-gradient(linear, left top, right top, #fd4b33, #ff6b42);
    background: -webkit-linear-gradient(left, #fd4b33, #ff6b42);
    background: linear-gradient(left, #fd4b33, #ff6b42);
    box-shadow: 0px 0px 10px rgba(195, 22, 0, 0.4);
    transform: rotate(45deg);
    -webkit-animation-name: bounceIn;
    -moz-animation-name: bounceIn;
    -o-animation-name: bounceIn;
    animation-name: bounceIn;
    -webkit-animation-duration: 18s;
    -moz-animation-duration: 18s;
    -o-animation-duration: 18s;
    animation-duration: 18s;
    -webkit-animation-timing-function: linear;
    -moz-animation-timing-function: linear;
    -o-animation-timing-function: linear;
    animation-timing-function: linear;
    -webkit-animation-fill-mode: both;
    -moz-animation-fill-mode: both;
    -o-animation-fill-mode: both;
    animation-fill-mode: both;
    -webkit-animation-iteration-count: infinite;
    -moz-animation-iteration-count: infinite;
    -o-animation-iteration-count: infinite;
    animation-iteration-count: infinite;
}

.cube1 {
    width: 6.8rem;
    height: 6.8rem;
    left: auto;
    right: -5%;
    top: 20%;
    /*top: 18%;*/
}

.cube2 {
    width: 2rem;
    height: 2rem;
    left: 0;
    top: 10%;
    -webkit-animation-delay: 3s;
    -moz-animation-delay: 3s;
    -o-animation-delay: 3s;
    animation-delay: 3s;
    /*top: 33%;*/
}

.cube3 {
    width: 2.5rem;
    height: 2.5rem;
    left: 37%;
    top: 18%;
    -webkit-animation-delay: 7s;
    -moz-animation-delay: 7s;
    -o-animation-delay: 7s;
    animation-delay: 7s;
    /*top: 47%;*/
}

.cube4 {
    width: 4.6rem;
    height: 4.6rem;
    left: 57%;
    top: 11%;
    -webkit-animation-delay: 11s;
    -moz-animation-delay: 11s;
    -o-animation-delay: 11s;
    animation-delay: 11s;
    /*top:  58.4%;*/
}

.cube5 {
    width: 4.6rem;
    height: 4.6rem;
    left: -1%;
    top: 6%;
    -webkit-animation-delay: 17s;
    -moz-animation-delay: 17s;
    -o-animation-delay: 17s;
    animation-delay: 17s;
    /*top: 67.4%;*/
}

@keyframes bounceIn {
    0%,
    4%,
    8%,
    12%,
    16%,
    20% {
        animation-timing-function: cubic-bezier(.215, .61, .355, 1)
    }
    0% {
        opacity: 0;
        transform: scale3d(.3, .3, .3) rotate(45deg);
    }
    4% {
        transform: scale3d(1.1, 1.1, 1.1) rotate(45deg);
    }
    8% {
        transform: scale3d(.9, .9, .9) rotate(45deg);
    }
    12% {
        opacity: 1;
        transform: scale3d(1.03, 1.03, 1.03) rotate(45deg);
    }
    16% {
        transform: scale3d(.97, .97, .97) rotate(45deg);
    }
    20% {
        opacity: 1;
        transform: scaleX(1) rotate(45deg);
        top: 0;
    }
    80% {
        opacity: 1;
        top: 60%;
    }
    to {
        transform: scaleX(1) rotate(360deg);
        top: 80%;
        opacity: 0;
    }
}

